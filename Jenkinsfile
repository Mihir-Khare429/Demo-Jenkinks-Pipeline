      steps {
          echo 'Under the Building stage'
      }
    }
    
    stage("test") {
        
      steps {
          echo 'Under the Testing stage'
      }
    }
    
    stage("deploy") {
        
      steps {
            echo 'Under the Deploying stage'
      }
    }
  }
}
  
